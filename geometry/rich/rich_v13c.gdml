	<variable name="pmt_rot_x_addend" value="-13.047"/>
	<variable name="pmt_rot_y_addend" value="-13.477"/>

	<!-- Small frame shift along the beam (tune it by yourself) -->
	<variable name="small_frame_shift" value="80"/>

</define>

