    <TransportWindow value="30"/>
    <RawFile value=""/>
    <MbsServerKind value="Stream"/>
    <CalibrFile value=""/>
    <MbsFileSizeLimit value="110"/>
    <SpillRoc value="0"/>
    <SpillAux value="0"/>
    <CalibrationPeriod value="5"/>
    <CalibrationLength value="0.5"/>
  </Application>
  <Module name="RocComb">
     <Ratemeter name="*" debug="true" interval="5" width="5" prec="2"/>
  </Module>
 </Context>
</dabc>
