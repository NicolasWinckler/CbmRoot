<define>
<!-- Internal variables - do not change until you know what it is -->
	<position name="central" x="0" y="0" z="0" unit="mm"/>
	<rotation name="identity" x="0" y="0" z="0" unit="degree"/>
	<variable name="DEGtoRAD" value="1.74532925199433E-02"/>
	<variable name="entrance_width_addend" value="2"/>

<!-- User variables (in mm and deg) -->

<!--	Always check the result visually. Volumes may intersect when you change values.
	Also use gGeoManager->CheckOverlaps(), gGeoManager->CheckGeometry(), gGeoManager->CheckGeometryFull()
	Total length of RICH is (RICH_length + 2*RICH_covering_thickness).
	Total length os rich1 keep-volume is 10cm more - there is a cylindrical part od the RICH pipe section included.
	Positive direction is towards beam axis for mirror and pmt X and Y rotations.
	Take care when rotating the pipe. The more the angle, the longer the pipe.
    At some point it may intersect other detectors and/or mirrors (but not covering or gas volumes - this is parametrized).
	Take care about the position of the small frame.
	'small_frame_shift' variable is used to shift the small frame so that it does not overlap with anything.
-->

	<!-- RICH size and position -->
	<variable name="RICH_trapezoid_angle" value="116"/>
	<variable name="RICH_entrance_width" value="2551.17"/>
	<variable name="RICH_exit_width" value="5136"/>
	<variable name="RICH_height" value="3958"/>
	<variable name="RICH_length" value="1890"/>
	<variable name="RICH_covering_thickness" value="5"/>

	<!-- RICH pipe size and position -->
	<variable name="RICH_pipe_entrance_r" value="103.5"/>
	<variable name="RICH_pipe_exit_r" value="205"/>
	<variable name="RICH_pipe_entrance_thickness" value="3"/>
	<variable name="RICH_pipe_exit_thickness" value="5"/>
	<variable name="RICH_pipe_angle" value="0"/>

	<!-- RICH mirrors size and position -->
	<variable name="RICH_position_from_IP" value="1800"/>
	<variable name="RICH_mirror_position_from_IP" value="3500"/>
	<variable name="RICH_mirror_r" value="3000"/>
	<variable name="RICH_mirror_thickness" value="6"/>
	<variable name="RICH_mirror_Y_shift" value="880"/>
	<variable name="RICH_mirror_angle" value="-1.5578"/>
	<variable name="mirror_cont_supp_addend" value="81"/>

	<!-- RICH PMT size and position -->
	<variable name="pmt_width" value="1000"/>
	<variable name="pmt_height" value="600"/>
	<variable name="pmt_thickness" value="0.5"/>
	<variable name="T" value="1500"/>
	<variable name="pmt_pos_x_addend" value="0"/>
