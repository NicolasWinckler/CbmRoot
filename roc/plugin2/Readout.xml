<?xml version="1.0"?>
<dabc version="1">
 <Context name="Readout">
  <Run>
    <lib value="libDabcMbs.so"/>
    <lib value="libDabcRoc.so"/>
    <logfile value="Readout.log"/>
  </Run>
  <Application class="roc::Readout">
    <SyncScaleDown value="0"/>
    <NumRocs value="1"/>
    <MeasureADC value="false"/>
    <RocIp0 value="cbmtest01"/>
    <RocFebs0 value="Feb1nxGenD,Feb1nxGenD"/>
    <BufferSize value="65536"/>
    <NumBuffers value="100"/>
    <FlushTimeout value="2"/>
    <TransportWindow value="30"/>
    <RawFile value=""/>
    <MbsFileSizeLimit value="110"/>
    <MbsServerKind value="Stream"/>
    <EpicsStreamNode value=""/>
  </Application>
  <Module name="RocComb">
     <Ratemeter name="*" debug="true" interval="3" width="5" prec="2"/>
  </Module>
 </Context>
</dabc>
