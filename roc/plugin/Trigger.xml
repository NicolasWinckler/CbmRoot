  </Run>
  <Application class="roc::Readout">
    <DoCalibr value="0"/>
    <SyncScaleDown value="3"/>
    <NumRocs value="1"/>
    <RocIp0 value="cbmtest11"/>
    <RocFebs0 value="Feb1nxGenC"/>
